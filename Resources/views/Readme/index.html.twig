<!DOCTYPE html>
    <html>
    <head>
        <meta charset="UTF-8" />
        <title>{{ title }}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <style type="text/css">
            ul {
                list-style: none;
                padding: 0;
            }

            .directory {
                background-image: url('data:image/svg+xml;base64,');
                display: inline-block;
                width: 16px;
                height: 16px;
                background-size: contain;
            }

            .file {
                background-image: url('data:image/svg+xml;base64,');
                display: inline-block;
                width: 16px;
                height: 16px;
                background-size: contain;
            }
        </style>
    </head>
    <body>
        <h1>{{ path }}</h1>
        <ul>
            {% for directory in directories %}
                <li>
                    <i class="directory"></i>
                    <a href="?path={{ directory.path }}">{{ directory.name }}</a>
                </li>
            {% endfor %}
            {% for file in files %}
                <li>
                    <i class="file"></i>
                    <a href="?path={{ file.path }}">{{ file.name }}</a>
                </li>
            {% endfor %}
        </ul>
    </body>
</html>
